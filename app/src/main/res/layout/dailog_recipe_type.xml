<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.recipie.rahulrecipie.activity.MainActivity">

    <data class="RecipeTypeDailogBinding">

        <import
            alias="ViewModel"
            type="com.recipie.rahulrecipie.viewmodel.DailogRecipeTypeViewModel" />

        <variable
            name="viewModel"
            type="ViewModel" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:orientation="vertical">

        <ListView
            android:id="@+id/listviewRecipe"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="3dp"
            android:layout_marginTop="3dp"></ListView>
    </LinearLayout>
</layout>

